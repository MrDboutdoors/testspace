		<SetToDellServiceTag>1</SetToDellServiceTag>
	</ComputerName>
	<LicenseKey>
		<Enabled>1</Enabled>
		<PrintToFile>C:\Deploy-PC\licensekey.txt</PrintToFile>
	</LicenseKey>
	<WindowsUpdates>
		<Enabled>1</Enabled>
