	<UserAccount>
		<Enabled>1</Enabled>
		<DisableRootAdmin>1</DisableRootAdmin>
		<CsvFilePath>.\UserAccount.csv</CsvFilePath>
	</UserAccount>
	<PowerConfig>
		<Enabled>1</Enabled>
		<MonitorTimeout>0</MonitorTimeout>
		<DiskTimeout>0</DiskTimeout>
		<StandbyTimeout>0</StandbyTimeout>
		<HibernateTimeout>0</HibernateTimeout>
	</PowerConfig>
	<Software>
		<Enabled>1</Enabled>
		<RemoveDefaultPackages>1</RemoveDefaultPackages>
		<AdobeReader>0</AdobeReader>
		<AdobeFlash>0</AdobeFlash>
		<GoogleChrome>0</GoogleChrome>
		<MozillaFirefox>0</MozillaFirefox>
		<AdblockPlusIE>0</AdblockPlusIE>
		<AdblockPlusChrome>0</AdblockPlusChrome>
		<AdblockPlusFirefox>0</AdblockPlusFirefox>
		<Java>0</Java>
		<Office365Business>0</Office365Business>
		<Office365ProPlus>0</Office365ProPlus>
		<SevenZip>0</SevenZip>
		<Winrar>0</Winrar>
		<NotepadPlusPlus>0</NotepadPlusPlus>
		<SysInternals>0</SysInternals>
		<ProcMon>0</ProcMon>
		<VCRedist2010>0</VCRedist2010>
		<VCRedist2008>0</VCRedist2008>
		<VCRedist20152019>0</VCRedist20152019>
